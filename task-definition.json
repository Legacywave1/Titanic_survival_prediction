{
  "family": "Titanic_prediction_task",
  "containerDefinitions": [
    {
      "name": "titanic-api",
      "image": "269481292162.dkr.ecr.us-east-1.amazonaws.com/titanic-api",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 8000,
          "hostPort": 8000,
          "protocal": "tcp"
        }
      ],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "ecs/Titanic_prediction_task",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-multiline-pattern": "^[DEBUG|INFO|WARNING|ERROR]"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::269481292162:role/ecsTaskExecutionRole"
}
